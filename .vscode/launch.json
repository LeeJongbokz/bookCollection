{
    {
        "version": "0.2.0",
        "configurations": [
            {
                "name": "Launch Program",
                "program": "${workspaceFolder}/src/app.js",
                "outFiles": [ "${workspaceRoot}/.compiled/**/*.js" ],
                "request": "launch",
                "skipFiles": [
                    "<node_internals>/**"
                ],
                "env":{
                    "DEBUG": null
                },
                "type": "pwa-node"
            },
            
            {
                "name": "Launch Babel",
                "type": "node",
                "request": "launch",
                "program": "${workspaceRoot}/app.js",
                "stopOnEntry": false,
                "args": [],
                "cwd": "${workspaceRoot}",
                "preLaunchTask": null,
                "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/babel-node",
                "runtimeArgs": [
                    "--nolazy",
                    "--require",
                    "babel-register"
                ],
                "env": {
                    "DEBUG": null,
                    "NODE_ENV": "development",
                    "BABEL_ENV": "debug"
                },
                "console": "internalConsole",
                "sourceMaps": true,
                "outFiles": [
                ]
            },
            {
                "name": "Attach",
                "type": "node",
                "request": "attach",
                "port": 5858,
                "address": "localhost",
                "restart": false,
                "sourceMaps": false,
                "outFiles": [],
                "localRoot": "${workspaceRoot}",
                "remoteRoot": null
            },
            {
                "name": "Attach to Process",
                "type": "node",
                "request": "attach",
                "processId": "${command.PickProcess}",
                "port": 5858,
                "sourceMaps": false,
                "outFiles": []
            }
        ]
    }